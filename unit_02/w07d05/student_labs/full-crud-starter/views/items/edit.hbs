<h1>Edit {{item.name}}</h1>
<form action="/users/{{user.id}}/items/{{item.id}}?_method=PATCH" method="POST">
  <div>
    <label for="name">
      Item Name
    </label>
    <input type="text" name="first_name" value="{{user.first_name}}"/>
  </div>
  <div>
    <input type="submit" value="Update Item"/>
  </div>
</form>
